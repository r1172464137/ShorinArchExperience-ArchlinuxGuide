      "output": "#c4c6d0"
    },
  },
  "modules": [
    "break",
    {
      "type": "os", //这是哪个module
      "key": "OS", //module名字的显示
      // "keyColor": "#00ff00",      //module名字颜色
      // "format": "{name} {version}",    //具体内容
      "keyColor": "#abc7ff",
   
    },
    {
      "type": "kernel",
      "key": " ├   KER ",
      "keyColor": "#abc7ff",

    },
    {
      "type": "packages",
      "key": " ├   PAK ",
      "format": "{all}",
