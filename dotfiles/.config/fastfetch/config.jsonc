    },
    {
      "type": "title",
      "key": " └   USR ",
      "keyColor": "#abc7ff",
    },
    "break",
    "break",
    {
      "type": "wm",
      "key": "WM",
      "keyColor": "#ddbce0",
   },
    {
      "type": "de",
      "key": " ├ 󱈹  DES ",
      "keyColor": "#ddbce0",
     //"outputColor": "#ddbce0"
    },
    {
      "type": "shell",
      "key": " ├   SHE ",
      "keyColor": "#ddbce0",
      //"outputColor": "#ddbce0"
    },
    {
      "type": "terminal",
      "key": " ├   TER ",
      "keyColor": "#ddbce0",
      //"outputColor": "#ddbce0"
    },
    {
      "type": "terminalfont",
      "key": " └   TFO ",
      "keyColor": "#ddbce0",
      //"outputColor": "#ddbce0"
    },
    "break",
    "break",
    {
      "type": "host",
      "key": "PC ",
      "keyColor": "#dae2f9",
      //"outputColor": "#bec6dc"
    },
    {
      "type": "cpu",
      "key": " ├   CPU ",
      "keyColor": "#dae2f9",
      //"outputColor": "#bec6dc"
    },
    {
