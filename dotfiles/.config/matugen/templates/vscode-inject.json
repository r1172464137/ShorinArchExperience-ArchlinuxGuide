  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "{{colors.secondary_fixed_dim.default.hex}}99",
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
            "variable",
            "variable.language",
            "variable.name",
            "variable.other",
            "variable.parameter"
        ],
        "settings": {
            "foreground": "{{colors.primary.default.hex}}"
        }
      }
    ]
  }
}
