  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment"],
        "settings": { "foreground": "{{colors.outline.dark.hex}}" }
      },
      {
        "scope": ["keyword"],
        "settings": { "foreground": "{{colors.primary.dark.hex}}" }
      },
      {
        "scope": ["string"],
        "settings": { "foreground": "{{colors.tertiary.dark.hex}}" }
      },
      {
        "scope": ["constant", "constant.language", "constant.numeric"],
        "settings": { "foreground": "{{colors.secondary.dark.hex}}" }
      },
      {
        "scope": ["variable"],
        "settings": { "foreground": "{{colors.on_surface.dark.hex}}" }
      },
      {
        "scope": ["entity.name.function"],
        "settings": { "foreground": "{{colors.primary.dark.hex}}" }
      },
      {
        "scope": ["entity.name.class", "support.type"],
        "settings": { "foreground": "{{colors.secondary.dark.hex}}" }
      },
      {
        "scope": ["invalid"],
        "settings": { "foreground": "{{colors.error.dark.hex}}" }
      },
      {
        "scope": ["markup.heading"],
        "settings": { "foreground": "{{colors.primary.dark.hex}}", "fontStyle": "bold" }
      },
      {
        "scope": ["entity.name.tag.yaml", "punctuation.definition.block.sequence.item.yaml"],
        "settings": { "foreground": "{{colors.on_surface.dark.hex}}" }
      },
      {
        "scope": ["source.yaml string.unquoted", "source.yaml string.quoted"],
        "settings": { "foreground": "{{colors.tertiary.dark.hex}}" }
      },
      {
        "scope": ["constant.language.boolean.yaml", "constant.language.null.yaml"],
        "settings": { "foreground": "{{colors.secondary.dark.hex}}" }
      },
      {
        "scope": ["punctuation.separator.key-value.mapping.yaml"],
        "settings": { "foreground": "{{colors.on_surface.dark.hex}}" }
      },
      {
        "scope": ["entity.name.tag.toml", "support.type.property-name.toml"],
        "settings": { "foreground": "{{colors.on_surface.dark.hex}}" }
      },
      {
        "scope": ["string.quoted.single.basic.line.toml", "string.quoted.double.basic.line.toml"],
        "settings": { "foreground": "{{colors.tertiary.dark.hex}}" }
      },
      {
        "scope": ["constant.language.boolean.toml", "constant.numeric.toml"],
        "settings": { "foreground": "{{colors.secondary.dark.hex}}" }
      },
      {
        "scope": ["meta.object-literal.key", "support.type.property-name.json"],
        "settings": { "foreground": "{{colors.on_surface.dark.hex}}" }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "variable": { "foreground": "{{colors.on_surface.dark.hex}}" },
      "parameter": { "foreground": "{{colors.on_surface_variant.dark.hex}}" },
      "property": { "foreground": "{{colors.secondary.dark.hex}}" },
      "function": { "foreground": "{{colors.primary.dark.hex}}" },
      "string": { "foreground": "{{colors.tertiary.dark.hex}}" },
      "number": { "foreground": "{{colors.secondary.dark.hex}}" },
      "keyword": { "foreground": "{{colors.primary.dark.hex}}" },
      "comment": { "foreground": "{{colors.outline.dark.hex}}" }
    }
  }
}

